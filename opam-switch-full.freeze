      "build"
      "-p"
      name
      "-j"
      jobs
      "@install"
      "@runtest" {with-test}
      "@doc" {with-doc}
    ]
  ]
  dev-repo: "git+https://github.com/ocaml/camlp-streams.git"
  url {
    src: "https://github.com/ocaml/camlp-streams/archive/v5.0.1.tar.gz"
